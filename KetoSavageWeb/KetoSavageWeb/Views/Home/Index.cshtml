
@{
    ViewBag.Title = "KetoSavage Client Portal";
}
@Html.DevExpress().Panel(p =>
{
    p.Name = "mainContentPanel";
    p.SetContent(() =>
    {
        if(Request.IsAuthenticated)
        {
            ViewContext.Writer.Write("Home/Index landing page");

        }
        else
        {
            Html.RenderPartial("_userLoginForm");
        }
    });
}).GetHtml()


