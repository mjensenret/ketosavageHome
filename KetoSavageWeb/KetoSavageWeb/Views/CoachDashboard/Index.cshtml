@{
    ViewBag.Title = "Dashboard";
}
<div class="row">
    <div class="col-sm-12">
        @Html.Action("newsCarousel", "ManageNews")
    </div>
</div>

<div class="row">
    <div class="col-sm-12 col-md-6 col-lg-3">
        @Html.Action("ClientRenewalGrid", "CoachCharts")
    </div>
    <div class="col-sm-12 col-md-6 col-lg-9">
        <div class="row">
            <div class="col-sm-12 col-md-6 col-lg-4">
                @Html.Action("TopPerformers", "CoachCharts")
            </div>
            <div class="col-sm-12 col-md-6 col-lg-4">
                @Html.Action("BottomPerformers", "CoachCharts")
            </div>
        </div>
        <div class="row">
            <div class="col-sm-12 col-md-6 col-lg-4">
                @Html.Action("TopPerformersLifetime", "CoachCharts")
            </div>
            <div class="col-sm-12 col-md-6 col-lg-4">
                @Html.Action("BottomPerformersLifetime", "CoachCharts")
            </div>

        </div>
    </div>

</div>
